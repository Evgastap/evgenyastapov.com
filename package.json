{
  "name": "react-tailwind-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@deadcoder0904/react-typical": "^0.1.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.21.1",
    "framer-motion": "^4.1.17",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-meta-tags": "^1.0.1",
    "react-progressive-image": "^0.6.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.3.4",
    "typestat": "^0.5.0"
  },
  "scripts": {
    "purge:css": "cross-env NODE_ENV=production postcss src/tailwind.css -o src/index.css",
    "build:css": "postcss src/tailwind.css -o src/index.css",
    "watch:css": "postcss src/tailwind.css -o src/index.css -w",
    "start": "npm run build:css && react-scripts start",
    "build": "npm run purge:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack-env": "^1.16.0",
    "autoprefixer": "^10.2.6",
    "cross-env": "^7.0.2",
    "cssnano": "^5.0.4",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.3.1"
  }
}

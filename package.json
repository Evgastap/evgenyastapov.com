{
  "name": "react-tailwind-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "@deadcoder0904/react-typical": "^0.1.2",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "cssnano": "^5.0.17",
    "eslint-plugin-react": "^7.28.0",
    "framer-motion": "^4.1.17",
    "next": "^12.1.0",
    "next-on-netlify": "^3.0.1",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.7",
    "react": "latest",
    "react-dom": "latest",
    "type-fest": "^0.21.3",
    "typescript": "^4.3.4",
    "typestat": "^0.5.0"
  },
  "scripts": {
    "purge:css": "cross-env NODE_ENV=production postcss src/tailwind.css -o src/index.css",
    "build:css": "postcss src/tailwind.css -o src/index.css",
    "watch:css": "postcss src/tailwind.css -o src/index.css -w",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "start": "next start",
    "build": "next build",
    "postbuild": "next-on-netlify",
    "dev": "next dev",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.2.7",
    "@types/node": "^17.0.18",
    "@types/react": "^17.0.39",
    "@types/webpack-env": "^1.16.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.9.0",
    "find-cache-dir": "^3.3.2",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.23"
  }
}
